body{
	background: #f1f1f1;
}
h1,h2,h3,h4,h5,h6{
	margin: 0;
	font-family: '微软雅黑';
}

textarea{
	resize:none;
	min-height: 80px;
}
.btn a{
	color: #fff;
}
.btn.btn-default a{
	color: #203140;
}
.btn a:hover,.btn a:focus{
	text-decoration: none;
}
.wrapper{
	width: 800px;
	margin:auto;
}
.form_text{
	padding: 10px;
	overflow: auto;
}
.form_select{
	padding: 10px;
}
.form_select .dropdown{
	width: 78%;
	display: inline-block;
}
.form_select .select_item{
	width: 22%;
	display: inline-block;
	min-height: 34px;
	line-height: 34px;
}
.form_text span{
	display: inline-block;
	float: left;
	min-height: 34px;
	line-height: 34px;
}
.form_text span:nth-child(1){
	width: 22%;
}
.form_text span:nth-child(2){
	width: 78%;
}
.title{
	border-left: 4px solid #36b3b3;
	background: #f9f9f9;
}
.title h4{
	padding: 10px 15px;
	font-size: 16px;
}
/*登录*/
.header{
	color: #fff;
	padding: 20px 0;
}
.header{
	background: rgba(33,50,65,0.5);
	text-align: center;

}
.illustrate{
	padding: 10px 10px 0 10px;
	line-height: 30px;
	border-top: 1px solid #fff;
	font-size: 12px;
	color: #999;
}
.logo{
	margin-right: 40px;
}
#login_bg{
	background: url("../image/bg.jpg") no-repeat;
	background-size:100% 100%;
}
.login{
	width: 460px;
	padding: 20px;
	margin: 40px auto 20px auto;
	background: rgba(255,255,255,0.8);
	border-radius: 10px;
}
.login h4{
	text-align: center;
}
.login .login-header{
	padding-bottom: 20px;
	border-bottom: 1px solid #fff;
}
.login .login-content{
	padding: 10px 0;
}
.login .login-footer{
	padding-top: 20px;
	text-align: center;
	border-top: 1px solid #fff;
}
.login .login-footer .btn{
	margin: 0 10px;
}
/*登录*/
/*header*/
#header{
	background: url("../image/bg.jpg") 0 -140px;
	color: #fff;
	padding: 15px 0 15px 50px;
	position: fixed;
	left: 0;
	right: 0;
	top: 0;
	z-index: 9999;
	width: 100%;
}
#header #logo{
	height: 48px;
	margin-right: 30px;
}
#header .user{
	position: absolute;
	right: 30px;
	top: 26px;
	cursor: pointer;
}
#header .user a{
	color: #fff;
}
/*header*/
/*main*/
#main{
	height: 100%;
}
#nav{
	width: 200px;
	background: #293d51;
	position: fixed;
	left: 0;
	bottom: 0;
	top: 78px;
	z-index: 99;
	overflow: hidden;
}
#nav h5 a{
	color: #68809a;
	padding: 10px 0 10px 20px;
	display: block;
	text-decoration: none;
}
#nav .nav_box img{
	width: 20px;
	vertical-align: bottom;
	margin-right: 10px;
}
#nav .nav_child>div a{
	display: block;
	padding: 10px 0 10px 45px;
	color: #fff;
	cursor: pointer;
	background: #203140;
	border-left: 3px solid #203140;
}
#nav .nav_child>div a:hover,#nav .nav_child>div a.current{
	color: #36b3b3;
	text-decoration: none;
	background: #1a2835;
	border-color: #36b3b3;
}
#all{
	padding:95px 15px 0 215px;
}
.box{
	padding: 15px;
	background: #fff;
	margin-bottom: 15px;
}
.tools_bar{
	position: relative;
}
.tools_bar>div{
	display: inline-block;
}
.tools_bar>div+div{
	margin-left: 10px;
}
.tools_bar .tools_btn{
	position: absolute;
	right: 0;
	top: 0;
}
.tools_bar .tools_btn .btn{
	margin-left: 10px;
}
.page_name{
	background: #f9f9f9;
	font-size: 18px;
	text-align: center;
	border-bottom: 1px solid #ededef;
	padding: 20px 0;

}
.form_table{
	
}
.form_table table{
	margin-bottom: 0;
}
.form_table table td,.form_table table th{
	text-align: center;
}
.form_table table tr:hover td,.form_table table tr.active td{
	background: #f0ad4e;
	color: #fff;
}
.table_detail{
	border-top: 1px solid #ededef;
	padding-top: 15px;
	position: relative;
	display: none;
}
.table_detail .btn_box{
	text-align: center;
	padding: 15px 0;
	border-top: 1px solid #ededef;
	margin-top: 15px;
}
.btn_box .btn{
	margin: 0 10px;
}
.double_control{
	overflow: auto;
}
.double_control span{
	display: inline-block;
	float: left;
	padding:5px 0;
}
.double_control span:nth-child(odd){
	width: 10%;
	min-height: 34px;
	line-height: 34px;
	text-align: right;
}
.double_control span:nth-child(even){
	width: 40%;
}
.navigation{
	text-align: center;
}
.pagination > li > a, .pagination > li > span{
	color: #36b3b3;
}
.pagination > .active > a, .pagination > .active > span, .pagination > .active > a:hover, .pagination > .active > span:hover, .pagination > .active > a:focus, .pagination > .active > span:focus{
	background: #36b3b3;
	border-color: #36b3b3;
}
.modal{
	z-index: 9999;
}
.modal .dialog_form>div{
	overflow: auto;
}
.modal .dialog_form>div+div{
	padding-top: 10px;
}
.modal .dialog_form>div span{
	display: inline-block;
	float: left;
	height: 34px;
	line-height: 34px;
	width: 22%;
}
.modal .dialog_form>div input,.modal .dialog_form>div textarea{
	width: 78%;
	float: left;
}
